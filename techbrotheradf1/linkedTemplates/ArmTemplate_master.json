{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "techbrotheradf1"
		},
		"Lnk_azure_sql_tr_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Lnk_azure_sql_tr'"
		},
		"Lnk_copy_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Lnk_copy'"
		},
		"Lnk_copy_src_tlb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Lnk_copy_src_tlb'"
		},
		"lnk_achieve_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lnk_achieve'"
		},
		"Lnk_azure_sql_tr_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "techsql1.database.windows.net"
		},
		"Lnk_azure_sql_tr_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "techsql"
		},
		"Lnk_azure_sql_tr_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminsql"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "techbrotheradf1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Lnk_azure_sql_tr_password": {
						"value": "[parameters('Lnk_azure_sql_tr_password')]"
					},
					"Lnk_copy_connectionString": {
						"value": "[parameters('Lnk_copy_connectionString')]"
					},
					"Lnk_copy_src_tlb_connectionString": {
						"value": "[parameters('Lnk_copy_src_tlb_connectionString')]"
					},
					"lnk_achieve_connectionString": {
						"value": "[parameters('lnk_achieve_connectionString')]"
					},
					"Lnk_azure_sql_tr_properties_typeProperties_server": {
						"value": "[parameters('Lnk_azure_sql_tr_properties_typeProperties_server')]"
					},
					"Lnk_azure_sql_tr_properties_typeProperties_database": {
						"value": "[parameters('Lnk_azure_sql_tr_properties_typeProperties_database')]"
					},
					"Lnk_azure_sql_tr_properties_typeProperties_userName": {
						"value": "[parameters('Lnk_azure_sql_tr_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}